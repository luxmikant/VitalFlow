<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.vitalflow.hospital" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>VitalFlow AI-Assisted Hospital Command Center - Transform passive analytics into active operations</description>
    <author name="VitalFlow Team" email="team@vitalflow.dev" organization="VitalFlow" website="https://vitalflow.dev"/>
    <min-api-version>1.10</min-api-version>
    <source-location>
      <url>http://localhost:8765/index.html</url>
    </source-location>
    <icon></icon>
    <permissions>
      <permission>full data</permission>
    </permissions>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">VitalFlow Command Center</text>
    </resource>
  </resources>
</manifest>
